    {
      "name": "live-products",
      "description": "Allows access to resolve a graphql request",
      "statements": [
        {
          "effect": "allow",
          "actions": ["post", "get"],
          "resources": [
            "vrn:vtex.store-graphql:{{region}}:{{account}}:{{workspace}}:/_v/graphql"
          ]
        }
      ]
    }
  ]